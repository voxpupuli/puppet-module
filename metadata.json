{
  "name": "jorhett-puppet4",
  "version": "0.1.0",
  "author": "Jo Rhett",
  "summary": "Installs and configures Puppet 4 package, tools, and optionally agent",
  "license": "BSD-3-Clause",
  "source": "https://github.com/jorhett/puppet4-module",
  "issues_url": "https://github.com/jorhett/puppet4-module/issues",
  "project_page": "https://github.com/jorhett/puppet4-module",
  "tags": ["puppet4"],
  "operatingsystem_support": [
    {
      "operatingsystem":"RedHat",
      "operatingsystemrelease":[ "6","7" ]
    },
    {
      "operatingsystem":"CentOS",
      "operatingsystemrelease":[ "6","7" ]
    },
    {
      "operatingsystem":"OracleLinux",
      "operatingsystemrelease":[ "6","7" ]
    },
    {
      "operatingsystem":"Scientific",
      "operatingsystemrelease":[ "6","7" ]
    },
    {
      "operatingsystem":"Amazon",
      "operatingsystemrelease":[ "2015.09","2015.03","2014.09","2014.03" ]
    },
    {
      "operatingsystem":"Debian",
      "operatingsystemrelease":[ "6","7" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "16.10", "16.04", "15.10", "15.04", "14.10", "14.04", "13.10", "13.04", "12.10", "12.04" ]
    }
  ],
  "requirements": [
    { "name": "pe", "version_requirement": ">= 2015.2" },
    { "name": "puppet", "version_requirement": ">= 4.0.0" }
